[global_config]
  handle_size = 0
  suppress_multiple_term_dialog = True
  title_inactive_bg_color = "#9e9e9e"
  title_inactive_fg_color = "#574f4a"
  title_receive_bg_color = "#48b9c7"
  title_receive_fg_color = "#3b3633"
  title_transmit_bg_color = "#32a19a"
  title_transmit_fg_color = "#3b3633"
  window_state = maximise
[keybindings]
  reset_clear = <Primary><Shift>k
  split_horiz = <Primary>d
  split_vert = <Primary><Shift>d
[layouts]
  [[default]]
    [[[child1]]]
      command = ""
      directory = ""
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
  [[mx]]
    [[[child0]]]
      fullscreen = False
      last_active_term = a30e80c1-63b0-4e7d-b476-cc67923779b9
      last_active_window = True
      maximised = True
      order = 0
      parent = ""
      position = 0:0
      size = 1920, 1025
      title = jim@jim-xps: ~
      type = Window
    [[[child1]]]
      order = 0
      parent = child0
      position = 510
      ratio = 0.5
      type = VPaned
    [[[terminal2]]]
      order = 0
      parent = child1
      profile = mxbe
      type = Terminal
      uuid = a30e80c1-63b0-4e7d-b476-cc67923779b9
    [[[terminal3]]]
      order = 1
      parent = child1
      profile = mxfe
      type = Terminal
      uuid = 28739136-57eb-42f7-a303-cced955090be
[plugins]
[profiles]
  [[default]]
    background_color = "#002b36"
    background_darkness = 1.0
    background_type = transparent
    cursor_color = "#aaaaaa"
    foreground_color = "#839496"
    scrollback_infinite = True
  [[mxbe]]
    background_color = "#002b36"
    background_darkness = 1.0
    background_type = transparent
    cursor_color = "#aaaaaa"
    custom_command = env INIT_CMD="cd ~/Documents/code/maritimexchange-backend; npm start" zsh
    foreground_color = "#839496"
    scrollback_infinite = True
    use_custom_command = True
  [[mxfe]]
    background_color = "#002b36"
    background_darkness = 1.0
    background_type = transparent
    cursor_color = "#aaaaaa"
    custom_command = env INIT_CMD="cd ~/Documents/code/maritimexchange; npm start" zsh
    exit_action = hold
    foreground_color = "#839496"
    scrollback_infinite = True
    use_custom_command = True
